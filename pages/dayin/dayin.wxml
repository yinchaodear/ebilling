      <van-share-sheet
  show="{{ showShare }}"
  title="{{title}}"
  options="{{ options }}"
  bind:select="onSelect"
  bind:close="onClose"
/>    
<van-collapse value="{{ activeNames }}" accordion='true'  bind:change="onItemChange">
  <van-collapse-item title="开票项" name="1"  value ="最多添加8项">
  <view class="pannel-item" wx:for='{{ItemList}}'>
     <view class='pa-t30 pa-b20 panel-between item-center border-b'  >
        <view class='fo-26'>项目名</view>
        <input class='fo-26 text-right' bindinput="confirmitemList" data-index='{{index}}'  data-name="name" placeholder="填写项目名"  value="{{item.name}}" ></input>
         </view>
      <view class='pa-t30 pa-b20 panel-between item-center border-b'  >
        <view class='fo-26'>对应金额</view>
        <input class='fo-26 text-right' bindinput="confirmitemList" data-index='{{index}}' data-name="money" placeholder="填写对应金额" value="{{item.money}}"  ></input>
       
      </view>
      <view class='pa-t30 pa-b20 panel-between item-center border-b'  >
        <view class='fo-26'>税率(%)</view>
        <input class='fo-26  text-right' bindinput="confirmitemList"  data-index='{{index}}' data-name="tax" value="{{item.tax}}"  placeholder=""  ></input>
      </view>
      <view class='additem-small'   data-index='{{index}}' bindtap="deleteitem" >
        删除此项
      </view>
    </view>   
  </van-collapse-item>

     <view class='additem'  bindtap="additem" >
         添加开票项
      </view>
</van-collapse>

