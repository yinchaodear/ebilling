    <view wx:if='{{cancelimglist.length>0}}'>
    <view class='fo-30 ma-t30'>作废申请图片</view>
    <van-grid column-num="3" border="{{ false }}">
  <van-grid-item use-slot wx:for="{{ cancelimglist}}">
    <image
      style="width: 100%; height: 90px;"
      src="{{item}}" 
      bindtap="showimage"
      data-url="{{item}}"
